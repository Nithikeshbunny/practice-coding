#include <stdio.h>
int main()
{
    int i, x;
    printf("Enter number: ");
    scanf("%d", &x);
    for (i = 2; i < x; i++)
    {
        if (x % i == 0)
        {
            printf("It is not a prime number\n");
            return 0;
        }
    }
    printf("It is a prime number\n");
    return 0;
}
